{
    "name": "shadow81627/pocketpasta-api",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-redis": "5.3.*",
        "algolia/algoliasearch-client-php": "3.1.0",
        "doctrine/dbal": "3.1.2",
        "dyrynda/laravel-cascade-soft-deletes": "4.1.0",
        "enlightn/enlightn": "1.22.1",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.4",
        "guzzlehttp/guzzle": "7.3.0",
        "inertiajs/inertia-laravel": "0.4.4",
        "laravel-json-api/laravel": "1.0.0",
        "laravel/framework": "8.61.0",
        "laravel/horizon": "5.7.13",
        "laravel/jetstream": "2.4.0",
        "laravel/sanctum": "2.11.2",
        "laravel/scout": "9.2.9",
        "laravel/telescope": "4.6.4",
        "laravel/tinker": "2.6.1",
        "maatwebsite/excel": "3.1.33",
        "meilisearch/meilisearch-php": "0.19.1",
        "sentry/sentry-laravel": "2.8.0",
        "spatie/eloquent-sortable": "4.0.0",
        "spatie/laravel-activitylog": "4.1.1",
        "spatie/laravel-http-logger": "1.7",
        "spatie/laravel-responsecache": "7.1.0",
        "spatie/laravel-schemaless-attributes": "2.0.0",
        "spatie/laravel-sluggable": "3.1.0",
        "spatie/laravel-tags": "4.0.4",
        "staudenmeir/eloquent-eager-limit": "1.6.1",
        "staudenmeir/eloquent-has-many-deep": "1.14.3",
        "tightenco/ziggy": "1.4.1"
    },
    "require-dev": {
        "cweagans/composer-patches": "1.7.1",
        "facade/ignition": "2.13.1",
        "fakerphp/faker": "1.16.0",
        "laravel-json-api/testing": "1.0.0",
        "laravel/sail": "1.10.1",
        "mockery/mockery": "1.4.4",
        "nunomaduro/collision": "5.10.0",
        "pestphp/pest": "1.20.0",
        "pestphp/pest-plugin-laravel": "1.1",
        "phpunit/phpunit": "9.5.10",
        "spatie/phpunit-watcher": "1.23.2",
        "squizlabs/php_codesniffer": "3.6.0",
        "symplify/vendor-patches": "9.4.65"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "php artisan test:pest",
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch < /dev/tty"
        ],
        "lint": "phpcs",
        "lint:fix": "phpcbf",
        "enlightn": "php artisan enlightn --report",
        "pre-update-cmd": [
            "@php artisan telescope:publish --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
